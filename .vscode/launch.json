    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Local",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "runtimeExecutable": "/Users/baynes/.nvm/versions/node/v18.17.1/bin/nodemon",
            "program": "${workspaceFolder}/index_local.js",
            "outFiles": [
                "${workspaceFolder}/**/*.js"
            ],
            "env": {
                "PORT": "8000",
                "LOCAL": "true",
                "ENV_REGION": "eu-central-1",
                "TENANT_TABLE": "nelson-tenants",
                "TENANT_PROPS_BUCKET": "nelson-tenant-properties",
                "ACCESSKEY": "{{AWS Access Key}}",
                "SECRETKEY": "{{AWS Secret Key}}"
            }
        }
    ]
}